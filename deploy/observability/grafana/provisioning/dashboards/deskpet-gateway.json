{
  "uid": "deskpet-gateway",
  "title": "Deskpet Gateway Overview",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "timezone": "browser",
  "tags": [
    "deskpet",
    "gateway"
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Online Devices",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "deskpet_gateway_online",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Uptime (s)",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "deskpet_gateway_uptime_seconds",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Connect Rate (1m)",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(deskpet_gateway_connect_total[1m])",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Disconnect Rate (1m)",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(deskpet_gateway_disconnect_total[1m])",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "timeseries",
      "title": "Telemetry Rate (1m)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 5
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(deskpet_gateway_telemetry_total[1m])",
          "refId": "A",
          "legendFormat": "telemetry"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Ack Rate (1m)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 5
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(deskpet_gateway_ack_total[1m])",
          "refId": "A",
          "legendFormat": "ack"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Command Send Rate (1m)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 13
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(deskpet_gateway_command_send_total[1m])",
          "refId": "A",
          "legendFormat": "send"
        },
        {
          "expr": "rate(deskpet_gateway_command_send_ok_total[1m])",
          "refId": "B",
          "legendFormat": "send_ok"
        },
        {
          "expr": "rate(deskpet_gateway_command_send_fail_total[1m])",
          "refId": "C",
          "legendFormat": "send_fail"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Failures Rate (1m)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 13
      },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "rate(deskpet_gateway_callback_fail_total[1m])",
          "refId": "A",
          "legendFormat": "callback_fail"
        },
        {
          "expr": "rate(deskpet_gateway_auth_fail_total[1m])",
          "refId": "B",
          "legendFormat": "auth_fail"
        }
      ]
    }
  ]
}
